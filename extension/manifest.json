{
  "name": "The Boo Bot",
  "description": "The Boo Bot for social media helps reduce anxiety by blurring propaganda bots posts",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://twitter.com/", "https://hackforpeace-bots-boo.vercel.app/"],
  "action": {
    "default_title": "Boo Bot",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
